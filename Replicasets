apiVersion: apps/v1
kind: ReplicaSet
metadata:
  name: fmyapp-replicaset
  labels:
    app: myapp
    type: front-end
spec:
  template:

     metadata:
       labels:
         app: myapp
         type: frontend
     spec:
       containers:
       - name: php-redis
         image: gcr.io/google_samples/gb-frontend:v3
replicas: 3
Selector:
      matchLabels:
         type: front-end
